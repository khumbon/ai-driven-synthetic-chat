    "@tanstack/react-query": "^5.80.5",
    "axios": "^1.9.0",
    "crypto": "^1.0.1",
    "dotenv": "^16.5.0",
    "next": "15.3.2",
    "path": "^0.12.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.56.4",
